{
  "name": "schedule-tocal",
  "version": "0.1.0",
  "description": "Direct import of university schedule to Google Calendar",
  "main": "build/server.js",
  "scripts": {
    "dev": "npm run build && npm run start",
    "build": "webpack --config config/webpack/index.js",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "postbuild": "webpack --config config/webpack/server.js",
    "start": "node build/server.js",
    "start:prod": "cross-env NODE_ENV=production npm start",
    "deploy": "npm run build:prod && git commit -am 'rebuild' --allow-empty && git push heroku master",
    "test": "node_modules/.bin/karma start config/test/karma.conf.js",
    "lint": "node_modules/.bin/tslint \"src/**/**.ts*\""
  },
  "author": "Alexander Gazizov <alexandr.bbm@gmail.com>",
  "homepage": "https://schedule-to-cal.herokuapp.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexandr-bbm/schedule-to-cal.git"
  },
  "bugs": {
    "url": "https://github.com/alexandr-bbm/schedule-to-cal/issues"
  },
  "keywords": [
    "university schedule",
    "import to google calendar",
    "dom parser",
    "react",
    "redux",
    "typescript",
    "webpack",
    "isomorphic"
  ],
  "license": "MIT",
